        <dc:creator>Spiros Kousouris (spiros@suite5.eu)</dc:creator>
        <terms:abstract>An ontology for energy services, defined as an extension of SAREF to address lifecycle assessment parameters for the energy systems.</terms:abstract>
        <terms:description>An ontology for energy services, defined as an extension of SAREF to address lifecycle assessment (LCA) parameters for the energy systems, especially needed in renovation projects.</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:publisher>SUITE5</terms:publisher>
        <terms:title>Digital Construction - EnergySystems</terms:title>
        <vann:preferredNamespacePrefix>dices:</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/digitalconstruction/EnergySystems</vann:preferredNamespaceUri>
        <rdfs:label>Digital Construction Energy Systems</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://w3id.org/digitalconstruction"/>
    </owl:Ontology>
    


